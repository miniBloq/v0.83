	<initCode>
		<s>import math&#10;</s>
		<s>import pygame&#10;</s>
		<s>from miniSim import MiniSim&#10;</s>
		<s>from miniSim import MobileRobot&#10;</s>
		<s>miniSim = MiniSim()&#10;</s>
		<s>robot = miniSim.robot0&#10;&#10;</s>
		<s>def go():&#10;</s>
		<s>	miniSim.resetRobot(robot)</s>
	</initCode>
	<finalCode>
		<s>miniSim.go = go&#10;</s>
		<s>miniSim.run()</s>
	</finalCode>
	<build>
